{
  "manifest_version": 3,
  "name": "Does it FLoC",
  "version": "0.21",
  "description": "Scans website for FLoC API or FLoC blocking policy.",
  "action": {
    "default_popup" : "index.html",
    "default_icon" : "icons/icon16.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" },
  "permissions": [
    "activeTab"
  ],
  "offline_enabled": false
}